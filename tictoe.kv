						size_hint_y: None
					#	height: self.texture_size[1]
						padding: 0
						
				
					
					MDRoundFlatButton:
						id: reset_btn
						text: "Dark Theme"
						font_name: 'font'
						size_hint: 1, 0.1
						pos_hint: {"x": 0, "top":0}
						font_size: 34
						on_press: self.theme_cls.theme_style = "Dark"
					
					MDRoundFlatButton:
						id: reset_btn
						text: "Light Theme"
						font_name: 'font'
						size_hint: 1, 0.1
